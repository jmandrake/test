pipeline {
	agent any
		stages{
			stage('One'){
				steps {
					echo 'Hi, this is just a test from Jenkins'
				}
			}
			stage('Two'){
				steps {
					input('Do you want to proceed?')
				}
			}
			stage('Three'){
				when {
					not {
						branch "master"
					}
					steps {
						echo "Hello this is step Three"
					}
				}
			}
			stage('Four'){
				parallel {
					stage('Unit Test'){
						steps {
							echo "Running Unit Tests..."
						}
					}
					stage('Integration test'){
						agent {
							docker {
								reuseNoe false
								image 'unbutu'
							}
						}
						steps {
							echo 'Running the integraion step from stage Four'
						}
					}
				}
			}
		
		}


}